{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowControlledInfraOperations",
      "Effect": "Allow",
      "Action": [
        "ec2:*",
        "rds:*",
        "lambda:*",
        "ecs:*",
        "s3:*",
        "iam:Get*",
        "iam:List*",
        "iam:PassRole",
        "ce:GetCostAndUsage",
        "sts:GetCallerIdentity"
      ],
      "Resource": "*"
    }
  ]
}
